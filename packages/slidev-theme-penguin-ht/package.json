{
  "name": "slidev-theme-penguin-ht",
  "version": "0.1.3",
  "keywords": [
    "slidev-theme",
    "slidev"
  ],
  "engines": {
    "node": ">=14.0.0",
    "slidev": ">=0.19.3"
  },
  "author": "htharoldht <htharoldht@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git@github.com:htharoldht/slidev.git"
  },
  "license": "MIT",
  "bugs": "https://github.com/htharoldht/slidev/issues",
  "scripts": {
    "build": "slidev build example.md",
    "dev": "slidev example.md --open",
    "export": "slidev export example.md",
    "screenshot": "slidev export example.md --format png"
  },
  "dependencies": {
    "@slidev/types": "^0.35.4",
    "codemirror-theme-vars": "^0.1.1",
    "prism-theme-vars": "^0.2.4",
    "vite-svg-loader": "^3.3.0",
    "theme-vitesse": "^0.5.0"
  },
  "devDependencies": {
    "@slidev/cli": "^0.34.3",
    "@iconify-json/logos": "^1.1.5",
    "@iconify-json/ri": "^1.1.2",
    "prism-theme-vars": "^0.2.3",
    "theme-vitesse": "^0.4.10"
  },
  "//": "Learn more: https://sli.dev/themes/write-a-theme.html",
  "slidev": {
    "colorSchema": "both",
    "highlighter": "all",
    "defaults": {
      "aspectRatio": "16/9",
      "canvasWidth": 980,
      "fonts": {
        "sans": "Nunito Sans",
        "mono": "Fira Code"
      },
      "hightlighter": "prism"
    }
  }
}
